{% include 'new_header' %}

<top-element>
  <section class="padding-y-5xl padding-y-lg-6xl bg-brand-default">
    <div class="container">
      <div class="row row-gap-2xl">
        <div class="col-12 col-lg-7 d-flex flex-column row-gap-lg">
          {{ 'om-oss-top' | global_editable: class:'text-lg-p display-lg-heading' }}
        </div>
        <div class="col-12 col-lg-5 d-flex align-items-end justify-content-lg-end">
          {{ 'om-oss-cta-top' | global_editable: class: 'neutral-buttons-group d-flex flex-column gap-sm align-items-start flex-column flex-lg-row' }}
        </div>
      </div>
    </div>
  </section>
</top-element>

<section class="default-padding-section">
  <div class="container">
    <div class="row row-gap-xl">
      {% for i in (1..4) %}
        {% assign editable_name = 'om-oss-box-' | append: i %}
        {{ editable_name | global_editable: class: 'col-12 col-md-6 col-lg-3 d-flex flex-column row-gap-xs display-lg-heading text-md-p' }}
      {% endfor %}
    </div>

    <div class="row margin-top-2xl margin-top-lg-4xl">
      <div class="col-12">
        {{ 'om-oss-image' | global_editable: class: 'sub-page-image' }}
      </div>
    </div>
  </div>
</section>

<section class="default-padding-section">
  <div class="container">
    <div class="row">
      {{ 'om-oss-values' | global_editable: class: 'd-flex flex-column row-gap-lg col-12 col-lg-7' }}
    </div>

    <div class="row margin-top-4xl row-gap-xl">
      {% for i in (1..3) %}
        {% assign editable_name = 'om-oss-values-' | append: i %}
        <div class="col-12 col-lg-4">
          {{ editable_name | global_editable: class: 'bg-secondary radius-md d-flex flex-column row-gap-sm padding-y-2xl padding-x-xl padding-y-lg-3xl padding-x-lg-2xl h-100 display-xs-heading text-md-p' }}
        </div>
      {% endfor %}
    </div>
  </div>
</section>


<section class="default-padding-section" id="our-history">
  <div class="container">
    <fade-in threshold="0.5">
      <div class="row align-items-center row-gap-4xl">
        {{ 'om-oss-callout-text' | global_editable: class: 'col-12 col-lg-6 d-flex flex-column row-gap-lg padding-right-lg-2xl' }}
        {{ 'om-oss-callout-image' | global_editable: class: 'col-12 col-lg-6' }}
      </div>
    </fade-in>
  </div>
</section>


