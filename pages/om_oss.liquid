        {% for item in lists.foretags-policy-om-oss.rows %}
          <div
            class="gray-card bg-secondary radius-md padding-y-2xl padding-x-xl padding-y-lg-3xl padding-x-lg-2xl h-100">
            <div class="gray-card__image-wrapper margin-bottom-xl">
              <img src="{{ item.ikon }}" alt="{{ item.rubrik }}">
            </div>

            <div class="gray-card__content d-flex flex-column row-gap-sm">
              <p class="display-xs fw-bold">{{ item.rubrik }}</p>
              <p>{{ item.text }}</p>
            </div>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</section>

